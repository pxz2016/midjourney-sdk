{
  "name": "midjourney-sdk",
  "version": "2.1.0",
  "description": "MidJourney in Discord API for Node.js.",
  "packageManager": "pnpm@7.18.2",
  "type": "module",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "keywords": [
    "midjourney",
    "midjourney-api",
    "midjourney-sdk"
  ],
  "author": "LaiBaoYuan",
  "license": "MIT",
  "homepage": "https://github.com/LaiBaoYuan/midjourney-sdk/#readme",
  "bugs": {
    "url": "https://github.com/LaiBaoYuan/midjourney-sdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LaiBaoYuan/midjourney-sdk.git"
  },
  "scripts": {
    "dev": "nodemon --exec ts-node-esm --experimentalSpecifierResolution=node app.ts",
    "build": "tsc --noEmit && rollup --config rollup.config.ts --configPlugin esbuild",
    "format": "prettier --check --write .",
    "test": "vitest",
    "postinstall": "simple-git-hooks"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "ws": "^8.13.0"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@types/crypto-js": "^4.1.1",
    "@types/node": "^20.2.4",
    "@types/ws": "^8.5.5",
    "dotenv": "^16.0.3",
    "esbuild": "^0.17.19",
    "express": "^4.18.2",
    "lint-staged": "^13.2.2",
    "nodemon": "^3.0.1",
    "prettier": "^2.8.8",
    "rollup": "^3.23.0",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "simple-git-hooks": "^2.8.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "vitest": "^0.31.1"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": [
      "prettier --write --ignore-unknown"
    ]
  }
}
